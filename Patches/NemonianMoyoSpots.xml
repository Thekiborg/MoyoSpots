<?xml version="1.0" encoding="utf-8"?>
<Patch>

    <Operation Class="PatchOperationSequence">
        <operations>
            <!-- _______Female Torso Patch_______ -->
            <li Class="PatchOperationAdd">
                <xpath>Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Moyo"]/alienRace/generalSettings/alienPartGenerator/bodyAddons</xpath>
                    <value>
                        <li>
                            <path>NemonianMoyo/BodySpots/GlowFemale</path>
                            <bodyPart>Torso</bodyPart>
                            <drawnInBed>true</drawnInBed>
                            <inFrontOfBody>false</inFrontOfBody>
                            <defaultOffset>Center</defaultOffset>
                            <shaderType>MoteGlow</shaderType>
                            <drawSize>(1, 1)</drawSize>
                            <scaleWithPawnDrawsize>true</scaleWithPawnDrawsize>
                            <drawForMale>false</drawForMale>
                            <offsets>
                                <!-- <north>
                                    <layerOffset>-0.295</layerOffset>
                                </north> -->
                                <south>
                                    <layerOffset>-0.339</layerOffset>
                                </south>
                                <east>
                                    <layerOffset>-0.339</layerOffset>
                                </east>
                            </offsets>
